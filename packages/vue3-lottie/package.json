{
  "name": "@reslear/vue3-lottie",
  "version": "1.0.0",
  "license": "MIT",
  "author": "reslear",
  "type": "module",
  "module": "./dist/vue3-lottie.js",
  "main": "./dist/vue3-lottie.cjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vue3-lottie.js",
      "require": "./dist/vue3-lottie.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/*"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "lint": "eslint \"{packages,playground}/**/*.{ts,tsx,vue,js,jsx,html}\"",
    "prepublishOnly": "pnpm run build",
    "prettier": "npx prettier --write ."
  },
  "dependencies": {
    "@reslear/dotlottie-player-core": "^1.0.3",
    "lottie-web": "^5.12.2",
    "vite-plugin-lib-inject-css": "^2.1.1"
  },
  "devDependencies": {
    "@types/node": "22.7.9",
    "@vitejs/plugin-vue": "~5.1.4",
    "@vue/compiler-sfc": "~3.5.12",
    "typescript": "5.6.3",
    "vite": "~5.4.10",
    "vite-plugin-css-injected-by-js": "~3.5.2",
    "vite-plugin-dts": "~4.3.0",
    "vue": "~3.5.12"
  },
  "peerDependencies": {
    "vue": ">=3"
  },
  "engines": {
    "node": ">=12"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reslear/vue3-lottie.git"
  },
  "keywords": [
    "vue",
    "vue3",
    "lottie",
    "lottie-web",
    "lottie-light",
    "animation",
    "player",
    "lottiefiles",
    "dotlottie",
    ".lottie"
  ]
}